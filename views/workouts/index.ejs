<%- include('../partials/header') %>
<a href="/workouts/new">Add Workouts</a>
<table id="list">
    <thead>
      <tr>
        <th>Workout</th>
        <th>Date</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% workouts.forEach(function(w) { %>
        <tr>
          <td><%= w.name %></td>
          <td><%= w.date %></td>
          <td><a href="/workouts/<%= w._id %>">Exercises</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
<%- include('../partials/footer') %>